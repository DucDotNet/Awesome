{
  "build": {
    "content": [
      {
        "files": ["**/*.{md,yml}"],
        "exclude": ["_site/**"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "output": "_site",
    "template": ["default", "modern"],
    "globalMetadata": {
      "_appName": "DucDotNet",
      "_appTitle": "DucDotNet",
      "_enableSearch": true,
      "pdf": true
    }
  }
}
